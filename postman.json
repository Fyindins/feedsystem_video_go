      "request": {
        "method": "POST",
        "header": [
          { "key": "Content-Type", "value": "application/json" }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\n  \"username\": \"alice\",\n  \"password\": \"pass123\"\n}"
        },
        "url": {
          "raw": "{{host}}/user/register",
          "host": ["{{host}}"],
          "path": ["user", "register"]
        }
      }
    },
